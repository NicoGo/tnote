OUTA EQU 0xFFA2
LOADA EQU 0xFFDC
SEUIL EQU 0x006F
CNTMA EQU 0xFF00
PAS EQU 0x0010

ORG LOADA
START LOADA

LDQ 0,R0 // ligne 1
LDQ SEUIL,R2 // ligne 2
STW R0,@CNTMA

MAIN_PRGA
	LDQ 0,R0
	ADQ PAS,R0
        STW R0,@CNTMA	
BOUCLE	JSR @FONCTION // on fait les additionb
	CMP R0,R2
	JGE #MAIN_PRGA-$-2 // Jump
	STW R0,@CNTMA 
	STW R0,R2
	JMP #MAIN_PRGA-$-2 // Jump

FONCTION LDW R0,@CNTMA
	 ADQ 16,R0
	 RTS
