OUTA EQU 0xFFA2
LOADA EQU 0xFF10
SEUIL EQU 0x006F
CNTMA EQU 0xFF00
PAS EQU 0x0010

ORG LOADA
START LOADA


MAIN_PRGA
  LDQ 0,R0
  LDQ SEUIL,R2
  STW R0,@CNTMA

BOUCLE	JSR @FONCTION // on fait les additions
	CMP R0,R2
	JGE #BOUCLE-$-2 // Jump
	STW R0,@CNTMA
	JMP #BOUCLE-$-2 // Jump
  JEA @MAIN_PRGA

FONCTION LDW R0,@CNTMA
	 ADQ 16,R0
	 RTS
